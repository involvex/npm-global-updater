{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.dart_tool": true,
    "**/node_modules/**": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "bunfig",
    "dotenv",
    "eslintcache",
    "latestversion",
    "prebuild",
    "runls",
    "runupdate",
    "runupdateall",
    "showlatestversion",
    "showversion",
    "updateall"
  ]
}
